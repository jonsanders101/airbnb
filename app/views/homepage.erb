<body>
  <div id='home-container'>
  <form id="button" action="/spaces/new" method="get">
    <input id="list-space" type="submit" value="List a new space!">
  </form>
  <br>
  <form id="button" action="/spaces" method="get">
    <input type="submit" value="Book a trip!">
  </form>
  <form action="/sign-up" method="get">
    <input type="submit" value="Sign up!">
  </form>
  <form method="get" action="/sessions/new">
    <input type="submit" value="Sign in">
  </form>
  <form method="post" action="/sessions">
    <input type="hidden" name="_method" value="delete">
    <input type="submit" value="Sign out">
  </form>
  <% if current_user %>
    <form method="get" action="/users/<%= current_user.id %>">
      <input type="submit" value="My Account">
    </form>
  <% end %>
  </div>
</body>
